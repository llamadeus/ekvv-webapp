{
  "name": "ekvv-webapp",
  "version": "0.1.0",
  "description": "Unofficial web app for the electronic course catalogue of the University of Bielefeld",
  "author": "Amadeus Beckmann <amadeus@beckmann.io>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^8.12.0",
    "npm": "^6.4.1",
    "yarn": "^1.12.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "build": "yarn build:css && react-scripts build",
    "build:antd": "lessc ./src/styles/antd.less ./src/build/antd.css --js",
    "build:css": "concurrently \"yarn build:antd\" \"yarn build:tailwind\"",
    "build:tailwind": "tailwind build ./src/styles/tailwind.css -o ./src/build/tailwind.css",
    "dev": "cross-env HTTPS=true react-scripts start",
    "start": "cross-env NODE_ENV=production node ./src/server.js",
    "test": "react-scripts test"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "less": "^3.9.0",
    "tailwindcss": "^0.7.4"
  },
  "dependencies": {
    "antd": "^3.15.2",
    "http-proxy-middleware": "^0.19.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8"
  }
}
