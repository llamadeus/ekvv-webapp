{
  "name": "ekvv-webapp",
  "version": "0.1.0",
  "description": "Unofficial web app for the electronic course catalogue of the University of Bielefeld",
  "author": "Amadeus Beckmann <amadeus@beckmann.io>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^8.12.0",
    "npm": "^6.4.1",
    "yarn": "^1.12.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "build": "yarn build:css && react-app-rewired build",
    "build:antd": "lessc ./src/styles/antd.less ./src/build/antd.css --js",
    "build:css": "concurrently \"yarn build:antd\" \"yarn build:tailwind\"",
    "build:tailwind": "tailwind build ./src/styles/tailwind.css -o ./src/build/tailwind.css -c ./tailwind.config.js",
    "dev": "cross-env HTTPS=true react-app-rewired start",
    "start": "cross-env NODE_ENV=production node ./src/server.js",
    "test": "react-app-rewired test"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "customize-cra": "^0.2.12",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^1.3.1",
    "lato-font": "^3.0.0",
    "less": "^3.9.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "react-app-rewired": "^2.1.1",
    "tailwindcss": "^0.7.4"
  },
  "dependencies": {
    "antd": "^3.15.2",
    "connected-react-router": "^6.3.2",
    "express": "^4.16.4",
    "history": "^4.9.0",
    "history-prop-types": "^2.5.2",
    "http-proxy-middleware": "^0.19.1",
    "immutable": "^4.0.0-rc.12",
    "lodash-es": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0"
  }
}
