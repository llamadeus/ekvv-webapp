{
  "name": "ekvv-webapp",
  "version": "0.4.1",
  "description": "Unofficial web app for the electronic course catalogue of the University of Bielefeld",
  "author": "Amadeus Beckmann <amadeus@beckmann.io>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^12.6.0",
    "npm": "^6.9.0",
    "yarn": "^1.17.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "yarn build:css && cross-env REACT_APP_VERSION=$npm_package_version react-app-rewired build",
    "build:antd": "lessc ./src/styles/antd.less ./src/build/antd.css --js",
    "build:css": "concurrently \"yarn build:antd\" \"yarn build:tailwind\"",
    "build:tailwind": "tailwind build ./src/styles/tailwind.css -o ./src/build/tailwind.css -c ./tailwind.config.js",
    "dev": "cross-env HTTPS=true REACT_APP_VERSION=$npm_package_version-dev react-app-rewired start",
    "eslint": "eslint --ext .js,.jsx src/",
    "start": "cross-env NODE_ENV=development node ./src/server.js",
    "test": "react-app-rewired test"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "customize-cra": "^0.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "husky": "^3.0.9",
    "lato-font": "^3.0.0",
    "less": "^3.10.3",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.13.0",
    "react-app-rewired": "^2.1.4",
    "react-scripts": "3.2.0",
    "tailwindcss": "^1.1.3"
  },
  "dependencies": {
    "@mdi/font": "^4.6.95",
    "antd": "^3.25.3",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.6.1",
    "dexie": "^2.0.4",
    "express": "^4.16.4",
    "express-rate-limit": "^5.0.0",
    "history": "^4.10.1",
    "history-prop-types": "^2.5.2",
    "hoist-non-react-statics": "^3.3.0",
    "http-proxy-middleware": "^0.20.0",
    "ical2json": "^2.0.0",
    "immutable": "^4.0.0-rc.12",
    "immutable-prop-types": "^0.0.3",
    "lodash-es": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "rc-form": "^2.4.9",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.12.15",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.1",
    "reselect": "^4.0.0",
    "rrule": "^2.6.0"
  }
}
